{
  "expo": {
    "name": "Abba Gospel",
    "slug": "abba-gospel",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#032A63",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#032A63"
    },
    "androidStatusBar": {
      "backgroundColor": "#032A63",
      "barStyle": "light-content",
      "translucent": false
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "description": "Find news, gospel, your favorite music and TV shows. Abba Gospel provides you with the latest breaking news and videos straight from the gospel industry."
  }
}
